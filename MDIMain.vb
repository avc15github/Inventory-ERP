    Private Sub AutoBackupDatabase()
        'If Not IsServer() Then Exit Sub

        Try
            Me.Cursor = Cursors.WaitCursor

            'DeleteOldData()
            frmPasswordVerify.Backup = True

            Dim frmWait As New frmPasswordVerify
            frmWait.Show()

            Dim strBkWithYearUpZipFile As String = ""
            Dim strBkUpZipFile As String = ""
            Dim LatestBkupFolder As String = GetSettingAsPerId(ApplnSettingsIdx.GenBackupPath) & "\"
            Dim YearFolder As String = Mid$(Replace(SelYear, " ", ""), 1, 4)


            Dim strInvFile As String = Database & Format$(Now.Date.Day, "00") & Format$(Now.Date.Month, "00") & Now.Date.Year & Format$(Now.TimeOfDay.Hours, "00") & Format$(Now.TimeOfDay.Minutes, "00") & ".bak"
            Dim strZipFile As String = Database & Format$(Now.Date.Day, "00") & Format$(Now.Date.Month, "00") & Now.Date.Year & Format$(Now.TimeOfDay.Hours, "00") & Format$(Now.TimeOfDay.Minutes, "00") & ".zip"
            Dim cmdQry As SqlCommand
            Dim strFileName As String = ""
            Dim strBkPath As String = LatestBkupFolder


            strBkWithYearUpZipFile = Database & Format$(Now.Date.Day, "00") & Format$(Now.Date.Month, "00") & Now.Date.Year & Format$(Now.TimeOfDay.Hours, "00") & Format$(Now.TimeOfDay.Minutes, "00") & ".zip "
            strBkUpZipFile = Database & ".zip "

            'master= "Data Source=" & Server & ";Initial Catalog=Master;user=sa;pwd="
            strFileName = LatestBkupFolder & YearFolder & "\" & strInvFile

            'strFileName = txtFilename.Text

            Dim strBackupFldr As String = strBkPath & YearFolder

            If Not IO.Directory.Exists(strBackupFldr) Then
                IO.Directory.CreateDirectory(strBackupFldr)
            End If

            Dim strNetworkPath As String = strBkPath

            If Not IsServer() Then
                Dim FileNm As New IO.FileInfo(strFileName)

                strBkPath = strBkPath.Replace(":", "")
                strBackupFldr = strBackupFldr.Replace(":", "")
                strBkPath = FileNm.DirectoryName.Replace(":", "")
                strNetworkPath = "\\" & ServerMachName & "\" & strBkPath

                If Not IO.Directory.Exists(strNetworkPath) Then
                    IO.Directory.CreateDirectory(strNetworkPath)
                End If
            End If


            Query = "BACKUP DATABASE " & Database & " TO DISK = '" & strFileName & "' WITH INIT"
            cmdQry = New SqlCommand(Query, Common.NewSQLConnection)
            cmdQry.CommandTimeout = 500
            cmdQry.Connection.Open()
            cmdQry.ExecuteNonQuery()
            cmdQry.Connection.Close()



            'Dim strNetworkPath As String = strBkPath
            Dim BackupCopyToPath As String = ""
            Dim NetworkBackupFldr As String = ""

            If Not IsServer() Then
                'strBkPath = strBkPath.Replace(":", "")
                'strBackupFldr = strBackupFldr.Replace(":", "")
                'strNetworkPath = "\\" & ServerMachName & "\" & strBkPath
                'BackupCopyToPath = strNetworkPath
                'NetworkBackupFldr = "\\" & ServerMachName & "\" & strBackupFldr
                'strFileName = "\\" & ServerMachName & "\" & strFileName.Replace(":", "")

                strBkPath = strBkPath.Replace(":", "")
                strBackupFldr = strBackupFldr.Replace(":", "")
                strNetworkPath = "\\" & ServerMachName & "\" & strBkPath & "\"
                BackupCopyToPath = "\\" & ServerMachName & "\" & LatestBkupFolder.Replace(":", "")
                NetworkBackupFldr = "\\" & ServerMachName & "\" & strBackupFldr
                strFileName = "\\" & ServerMachName & "\" & strFileName.Replace(":", "")
            Else
                BackupCopyToPath = strBkPath
                NetworkBackupFldr = strBackupFldr
            End If

            If IsServer() Then
                If Not IO.Directory.Exists(strBackupFldr) Then
                    IO.Directory.CreateDirectory(strBackupFldr)
                End If
            Else
                If Not IO.Directory.Exists(NetworkBackupFldr) Then
                    IO.Directory.CreateDirectory(NetworkBackupFldr)
                End If
            End If

            If IsServer() Then
                IO.File.Copy(strFileName, LatestBkupFolder & Database & ".bak", True)
            Else
                IO.File.Copy(strFileName, strNetworkPath & "\" & Database & ".bak", True)
            End If

            If WinzipExists() Then
                'Dim strZipPath As String = System.Environment.GetFolderPath(Environment.SpecialFolder.ProgramFiles) & "\Winzip\Winzip32 -a " & NetworkBackupFldr & "\" & strBkWithYearUpZipFile & strFileName
                Dim strZipPath As String = Application.StartupPath & "\7za a -t7z " & NetworkBackupFldr & "\" & strBkWithYearUpZipFile & strFileName
                Call Shell(strZipPath, AppWinStyle.Hide, True)

                strZipPath = Application.StartupPath & "\7za a -t7z " & BackupCopyToPath & strBkUpZipFile & " " & strNetworkPath & Database & ".bak "
                Shell(strZipPath, AppWinStyle.Hide, True)

                'IO.File.Copy(NetworkBackupFldr & "\" & strBkWithYearUpZipFile, BackupCopyToPath & strBkUpZipFile, True)
            End If

            strInvFile = IO.Path.GetFileName(strFileName)

            ''Log backup details
            ''------------------------------------
            Query = "Insert Into DataBackupLog Values ('" & strInvFile.Replace(".bak", ".zip") & "',GetDate(), '" & Database & "','" & Environment.MachineName & "'," & UserId & ",'" & NetworkBackupFldr & "')"
            Common.ExecuteQuery(Query)
            ''------------------------------------

            If WinzipExists() Then
                If IO.File.Exists(strFileName) Then
                    Call IO.File.Delete(strFileName)
                End If

                If IsServer() Then
                    If IO.File.Exists(LatestBkupFolder & "\" & Database & ".bak") Then
                        IO.File.Delete(LatestBkupFolder & "\" & Database & ".bak")
                    End If
                Else
                    If IO.File.Exists(strNetworkPath & "\" & Database & ".bak") Then
                        IO.File.Delete(strNetworkPath & "\" & Database & ".bak")
                    End If
                End If
            End If


        Catch drvEx As System.IO.IOException
            'MsgBox("Invalid backup folder specified !", MsgBoxStyle.Critical)
        Finally
            frmPasswordVerify.Backup = False
            Me.Cursor = Cursors.Default
        End Try
    End Sub

