    Private Sub DayIsOpen()
        Query = "Select RecId, OpenCloseDt From DayOpenClose " _
            & " Where CloseDt = '0' " _
            & " Order By RecId Desc"
        Dim OpenDt As DataTable = Common.GetDataTable(Query)

        CurrentDayOpenDt = GetCurrentDate()
        CurrentDayOpenId = 0

        If CBool(Val(GetSettingAsPerId(ApplnSettingsIdx.GenDayClose))) Then
            If OpenDt.Rows.Count > 0 Then
                CurrentDayOpenDt = OpenDt.Rows(0)("OpenCloseDt")
                CurrentDayOpenId = OpenDt.Rows(0)("RecId")

                If GetCurrentDate() <> OpenDt.Rows(0)("OpenCloseDt") Then
                    Dim OpenClose As New frmDayOpenClose(frmDayOpenClose.OpenClose.Close)
                    OpenClose.ShowDialog()
                End If
            Else
                Dim OpenClose As New frmDayOpenClose(frmDayOpenClose.OpenClose.Open)
                OpenClose.ShowDialog()
            End If
        End If
    End Sub

