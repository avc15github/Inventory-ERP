    Private Sub checkChild(ByVal nod As TreeNode)
        For Each node As TreeNode In nod.Nodes
            If node.Nodes.Count > 0 Then
                node.ImageIndex = 1
                node.SelectedImageIndex = 1
                checkChild(node)
            End If
        Next
    End Sub

    Private Sub SetSystemCaption()
        Dim RecRow As DataRow
        Dim NewName As String = ""

        tvwMasters.LabelEdit = True
        tvwPurchase.LabelEdit = True
        'tvwSales.LabelEdit = True
        'tvwInventory.LabelEdit = True
        tvwAccounts.LabelEdit = True
        tvwInventoryReports.LabelEdit = True
        tvwAccountReports.LabelEdit = True
        tvwUtilities.LabelEdit = True
        tvwAdmin.LabelEdit = True

        Query = "Select SysId, SystemName, DefCaption From SystemCaption"
        Dim Table As DataTable = Common.GetDataTable(Query)

        For Each RecRow In Table.Rows
            For Each ndNode As TreeNode In tvwMasters.Nodes
                SetChildCaption(ndNode, RecRow("SystemName"), RecRow("DefCaption"), True)
            Next ndNode
        Next RecRow

        For Each RecRow In Table.Rows
            For Each ndNode As TreeNode In tvwInventoryReports.Nodes
                If ndNode.Name.ToUpper <> "NDRPTPROCPRODN" And ndNode.Name.ToUpper <> "NDRPTPROCDELAY" Then
                    SetChildCaption(ndNode, RecRow("SystemName"), RecRow("DefCaption"), False)
                End If
            Next ndNode
        Next RecRow

        For Each RecRow In Table.Rows
            For Each ndNode As TreeNode In tvwUtilities.Nodes
                SetChildCaption(ndNode, RecRow("SystemName"), RecRow("DefCaption"), False)
            Next ndNode
        Next RecRow

        tvwMasters.LabelEdit = False
        tvwPurchase.LabelEdit = False
        'tvwSales.LabelEdit = False
        'tvwInventory.LabelEdit = False
        tvwAccounts.LabelEdit = False
        tvwInventoryReports.LabelEdit = False
        tvwAccountReports.LabelEdit = False
        tvwUtilities.LabelEdit = False
        tvwAdmin.LabelEdit = False
    End Sub

    Private Sub SetChildCaption(ByVal nod As TreeNode, ByVal Name As String, ByVal Caption As String, ByVal IsMaster As Boolean)
        Dim NewName As String = ""

        If nod.Name.ToUpper <> "NDLOCATION" And nod.Name.ToUpper <> "NDSTOCKLOCATION" Then
            If nod.Text.ToUpper Like "*" & Name.ToUpper & "*" Then
                NewName = nod.Text.Replace(Name, Caption)
                nod.BeginEdit()
                nod.Text = NewName
                nod.EndEdit(False)
            End If
        End If

        For Each node As TreeNode In nod.Nodes
            If IsMaster Then
                If node.Text.ToUpper Like Name.ToUpper & "*" Then
                    NewName = node.Text.Replace(Name, Caption)
                    node.BeginEdit()
                    node.Text = NewName
                    node.EndEdit(False)
                End If
            Else
                If node.Name.ToUpper <> "NDSALESANALYSISBYTYPE" Then
                    If node.Text.ToUpper Like "*" & Name.ToUpper & "*" Then
                        NewName = node.Text.Replace(Name, Caption)
                        node.BeginEdit()
                        node.Text = NewName
                        node.EndEdit(False)
                    End If
                End If
            End If

            If node.Nodes.Count > 0 Then
                SetChildCaption(node, Name, Caption, IsMaster)
            End If
        Next
    End Sub

