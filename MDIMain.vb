    Private Sub MDIMain_Activated(ByVal sender As Object, ByVal e As System.EventArgs) Handles Me.Activated

        If FormLoaded Then
            FormLoaded = False

            Query = "Select DefScreen From Users Where User_Id = " & UserId
            Dim DefScreen As String = Common.ExecuteScalar(Query)
            Dim frmDispForm As Form

            If DefScreen.ToUpper = "PURCHASE" Then
                frmDispForm = New frmPurchase(frmPurchase.WithPO.No, frmPurchase.POWise.No, frmPurchase.BarcodeWise.No)
                DisplayForm(frmDispForm, "FRMPURCHASE", True)
                MDINaviBar.Visible = False

            ElseIf DefScreen.ToUpper = "PURCHASE RETURN" Then
                frmDispForm = New frmPurchaseReturn(frmPurchaseReturn.BarcodeWise.No)
                DisplayForm(frmDispForm, "FRMPURCHASERETURN", True)

            ElseIf DefScreen.ToUpper = "SALES" Then
                frmDispForm = New frmBill("SI")
                DisplayForm(frmDispForm, "FRMBILL", True)

            ElseIf DefScreen.ToUpper = "SALES RETURN" Then
                frmDispForm = New frmBill("SR")
                DisplayForm(frmDispForm, "FRMSALERTN", True)

            ElseIf DefScreen.ToUpper = "VOUCHER" Then
                frmDispForm = New frmVoucher("PY")
                DisplayForm(frmDispForm, "FRMVOUCHER", True)
            End If
        End If
    End Sub

