    Private Sub DeleteOldData()
        Me.Cursor = Cursors.WaitCursor

        If CBool(Val(GetSettingAsPerId(ApplnSettingsIdx.GenAutoBackup))) Then
            Query = "Select Backup_File, Backup_At " _
                & " From DataBackupLog " _
                & " Where Backup_DateTime <= DateAdd(day, " & -Val(GetSettingAsPerId(ApplnSettingsIdx.GenDeleteBackupAfterDays)) & ", Getdate())"
            Dim BackupData As DataTable = Common.GetDataTable(Query)
            Dim recRow As DataRow

            For Each recRow In BackupData.Rows
                If IO.File.Exists(recRow("Backup_At") & "\" & recRow("Backup_File")) Then
                    IO.File.Delete(recRow("Backup_At") & "\" & recRow("Backup_File"))
                End If
            Next recRow
        End If

        Me.Cursor = Cursors.Default
    End Sub

