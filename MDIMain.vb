    Private Sub MDIMain_FormClosing(ByVal sender As Object, ByVal e As System.Windows.Forms.FormClosingEventArgs) Handles Me.FormClosing
        If IsServer() Then
            If Not CloseApplication Then
                Call BackupDB()
                e.Cancel = True
            Else
                'CloseApplication = False
                Application.Exit()
            End If
        Else
            Try
                If CloseApplication Then
                    Application.Exit()
                Else
                    If MessageBox.Show("Are you sure you want to exit?", "ERP", MessageBoxButtons.YesNo) = DialogResult.Yes Then
                        Query = "Update UserLog Set LogOutDateTime = '" & Format$(Now, "MM/dd/yyyy HH:mm:ss tt") & "'" _
                        & " Where UserLog_Id = " & UserLogId
                        Common.ExecuteQuery(Query)

                        'CloseApplication = False
                        Application.Exit()
                    Else
                        e.Cancel = True
                    End If

                   
                End If


            Catch ex As Exception
                Exit Sub
            End Try
            'CloseApplication = True
            'Me.Close() : Application.Exit()
            ''''Rohit 05072017 For All Form Closing
            For Each frm As Form In Me.MdiChildren
                frm.Close()
            Next
            ''''End Rohit 05072017 For All Form Closing
        End If
    End Sub

