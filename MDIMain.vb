    Private Sub AddFavouritesButtons()
        Query = "Select Mod_Name + '00' + cast(Modules.Mod_Id as varchar(5)) ControlName, FavCaption, FavShortcut " _
           & " From UserRights " _
           & " Inner Join Modules On UserRights.Mod_Id = Modules.Mod_Id " _
           & " Where User_Id = " & UserId & " And IsFavourites = '1' And UserRights.AllowAccess = '1' And Modules.HasChild = '0' " _
           & " Order By FavSeq"
        favouritesmenu = Common.GetDataTable(Query)
        Dim btn As DevExpress.XtraEditors.SimpleButton
        Dim lastlocation As Point
        Dim firsttime As Boolean = True

        For Each dr As DataRow In favouritesmenu.Rows
            btn = New DevExpress.XtraEditors.SimpleButton
            grpShortcut.Controls.Add(btn)
            btn.Name = dr("ControlName")
            btn.Text = dr("FavCaption") & IIf((dr("FavShortcut") & "") <> "", " (" & dr("FavShortcut") & ")", "")
            btn.Tag = dr("ControlName").ToString().Split("00")(0)

            btn.LookAndFeel.UseDefaultLookAndFeel = False
            btn.LookAndFeel.SkinName = "Money Twins"

            If firsttime Then
                btn.Location = btnShortcut.Location
                lastlocation = btn.Location
                firsttime = False
            Else
                lastlocation = New Point(lastlocation.X, lastlocation.Y + 35)
                btn.Location = lastlocation
            End If

            btn.Size = btnShortcut.Size
            AddHandler btn.Click, AddressOf FavouriteButtonEvent
        Next

        btnShortcut.Visible = False
    End Sub

