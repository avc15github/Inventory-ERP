    Private Sub GetLedgerColumns()
        Query = "Select * From Ledger"
        Dim AllCols As DataTable = Common.GetDataTable(Query)
        Dim Cols As String = ""

        For Each col As DataColumn In AllCols.Columns
            Cols = Cols & vbTab & col.ColumnName
        Next col

        Clipboard.Clear()
        Clipboard.SetData(System.Windows.Forms.DataFormats.StringFormat, Cols)

        AllCols = Nothing
    End Sub

